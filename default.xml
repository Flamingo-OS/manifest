<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <!-- Remotes -->
    <remote name="github"
        fetch="ssh://git@github.com/"/>

    <remote name="gitlab"
        fetch="ssh://git@gitlab.com/"/>

    <remote name="clo_system"
        fetch="https://git.codelinaro.org/clo/la"
        revision="refs/tags/LA.QSSI.12.0.r1-07800-qssi.0"/>

    <remote name="clo_vendor"
        fetch="https://git.codelinaro.org/clo/la"
        revision="refs/tags/LA.UM.9.16.r1-10900-MANNAR.QSSI12.0"/>

    <remote name="aosp"
        fetch="https://android.googlesource.com"
        revision="refs/tags/android-12.1.0_r5"/>

    <remote name="flamingo"
        fetch="ssh://git@github.com/Flamingo-OS/"
        revision="A12.1"/>

    <remote name="flamingo-gitlab"
        fetch="ssh://git@gitlab.com/Flamingo-OS/"
        revision="A12.1"/>

    <remote name="flamingo-devices"
        fetch="ssh://git@github.com/FlamingoOS-Devices/"
        revision="A12.1"/>

    <remote name="aospa"
        fetch="https://github.com/AOSPA/"
        revision="sapphire"/>

    <remote name="aospa-blobs"
        fetch="https://github.com/ThankYouMario/"
        revision="sapphire"/>

    <!-- CLO repos -->
    <include name="system.xml"/>
    <include name="vendor.xml"/>

    <!-- Obsolete repos -->
    <include name="remove.xml" />

    <!-- Our forks / additions -->
    <include name="flamingo.xml" />
    <include name="lineage.xml" />
    <include name="proton.xml" />

</manifest>
